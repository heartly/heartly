{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "clean"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "outputs": []
    },
    "lint": {
      "dependsOn": ["typecheck"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["test:static"],
      "outputs": [],
      "inputs": ["src/**/*.ts"]
    }
  }
}
